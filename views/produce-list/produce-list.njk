<h2>Vegetables</h2>
<ul class="produce">
    {% for vegetable in month.vegetables %}
        <li class="produce__item">{{ vegetable }}</li>
    {% endfor %}
</ul>
<h2>Fruit</h2>
<ul class="produce">
    {% for fruit in month.fruit %}
        <li class="produce__item">{{ fruit }}</li>
    {% endfor %}
</ul>